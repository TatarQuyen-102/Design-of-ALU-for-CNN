#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01283438 .scope module, "tb_mux2to1" "tb_mux2to1" 2 3;
 .timescale -9 -12;
v01284740_0 .var "In0", 8 0;
v0128CDF0_0 .var "In1", 8 0;
v0128CE48_0 .net "Out", 8 0, L_0128CEF8; 1 drivers
v0128CEA0_0 .var "Sel", 0 0;
S_012834C0 .scope module, "dut" "mux2to1" 2 10, 3 1, S_01283438;
 .timescale 0 0;
P_0128B7A4 .param/l "N" 3 1, +C4<01001>;
v01282B48_0 .net "In0", 8 0, v01284740_0; 1 drivers
v01284638_0 .net "In1", 8 0, v0128CDF0_0; 1 drivers
v01284690_0 .alias "Out", 8 0, v0128CE48_0;
v012846E8_0 .net "Sel", 0 0, v0128CEA0_0; 1 drivers
L_0128CEF8 .functor MUXZ 9, v01284740_0, v0128CDF0_0, v0128CEA0_0, C4<>;
    .scope S_01283438;
T_0 ;
    %vpi_call 2 14 "$dumpfile", "mux2to1_tb.vcd";
    %vpi_call 2 15 "$dumpvars", 1'sb0, S_01283438;
    %vpi_call 2 17 "$display", "Testing 2-to-1 Multiplexer";
    %vpi_call 2 18 "$monitor", "Time=%0t | Sel=%b | In0=%b | In1=%b | Out=%b", $time, v0128CEA0_0, v01284740_0, v0128CDF0_0, v0128CE48_0;
    %movi 8, 1, 9;
    %set/v v01284740_0, 8, 9;
    %set/v v0128CDF0_0, 1, 9;
    %set/v v0128CEA0_0, 0, 1;
    %delay 10000, 0;
    %movi 8, 1, 9;
    %set/v v01284740_0, 8, 9;
    %set/v v0128CDF0_0, 1, 9;
    %set/v v0128CEA0_0, 1, 1;
    %delay 10000, 0;
    %movi 8, 341, 9;
    %set/v v01284740_0, 8, 9;
    %movi 8, 170, 9;
    %set/v v0128CDF0_0, 8, 9;
    %set/v v0128CEA0_0, 0, 1;
    %delay 10000, 0;
    %movi 8, 341, 9;
    %set/v v01284740_0, 8, 9;
    %movi 8, 170, 9;
    %set/v v0128CDF0_0, 8, 9;
    %set/v v0128CEA0_0, 1, 1;
    %delay 10000, 0;
    %set/v v01284740_0, 0, 9;
    %set/v v0128CDF0_0, 1, 9;
    %set/v v0128CEA0_0, 0, 1;
    %delay 10000, 0;
    %set/v v01284740_0, 0, 9;
    %set/v v0128CDF0_0, 1, 9;
    %set/v v0128CEA0_0, 1, 1;
    %delay 10000, 0;
    %vpi_call 2 28 "$display", "TEST COMPLETED";
    %vpi_call 2 29 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
