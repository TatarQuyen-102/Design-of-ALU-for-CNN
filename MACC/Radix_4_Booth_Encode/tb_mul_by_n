#! /c/iverilog/bin/vvp
:ivl_version "0.9.7 " "(v0_9_7)";
:vpi_time_precision - 12;
:vpi_module "system";
:vpi_module "v2005_math";
:vpi_module "va_math";
S_01332A80 .scope module, "tb_multiplier_by_n" "tb_multiplier_by_n" 2 3;
 .timescale -9 -12;
P_011A3D2C .param/l "N" 2 6, +C4<01000>;
P_011A3D40 .param/l "S" 2 7, +C4<01>;
v01333478_0 .var "A", 7 0;
v0133E260_0 .net "Y", 8 0, L_0133E468; 1 drivers
S_01332B08 .scope module, "dut" "multiplier_by_n" 2 14, 3 2, S_01332A80;
 .timescale 0 0;
P_01332C0C .param/l "N" 3 2, +C4<01000>;
P_01332C20 .param/l "S" 3 2, +C4<01>;
v013346A0_0 .net "A", 7 0, v01333478_0; 1 drivers
v01332B90_0 .alias "Y", 8 0, v0133E260_0;
v01333318_0 .net *"_s1", 0 0, L_0133E2B8; 1 drivers
v01333370_0 .net *"_s2", 8 0, L_0133E348; 1 drivers
v013333C8_0 .net *"_s6", 7 0, L_0133E3D8; 1 drivers
v01333420_0 .net *"_s8", 0 0, C4<0>; 1 drivers
L_0133E2B8 .part v01333478_0, 7, 1;
L_0133E348 .concat [ 8 1 0 0], v01333478_0, L_0133E2B8;
L_0133E3D8 .part L_0133E348, 0, 8;
L_0133E468 .concat [ 1 8 0 0], C4<0>, L_0133E3D8;
    .scope S_01332A80;
T_0 ;
    %vpi_call 2 18 "$dumpfile", "multiplier_by_n_tb.vcd";
    %vpi_call 2 19 "$dumpvars", 1'sb0, S_01332A80;
    %vpi_call 2 21 "$display", "Testing Multiplier by N (Shift Left by %d)", P_011A3D40;
    %vpi_call 2 22 "$monitor", "Time=%0t | A=%b (%0d) | Y=%b (%0d)", $time, v01333478_0, v01333478_0, v0133E260_0, v0133E260_0;
    %movi 8, 1, 8;
    %set/v v01333478_0, 8, 8;
    %delay 10000, 0;
    %movi 8, 2, 8;
    %set/v v01333478_0, 8, 8;
    %delay 10000, 0;
    %movi 8, 4, 8;
    %set/v v01333478_0, 8, 8;
    %delay 10000, 0;
    %movi 8, 128, 8;
    %set/v v01333478_0, 8, 8;
    %delay 10000, 0;
    %set/v v01333478_0, 1, 8;
    %delay 10000, 0;
    %movi 8, 85, 8;
    %set/v v01333478_0, 8, 8;
    %delay 10000, 0;
    %movi 8, 170, 8;
    %set/v v01333478_0, 8, 8;
    %delay 10000, 0;
    %vpi_call 2 33 "$display", "TEST COMPLETED";
    %vpi_call 2 34 "$finish";
    %end;
    .thread T_0;
# The file index is used to find the file name in the following table.
:file_names 4;
    "N/A";
    "<interactive>";
    "mul_by_n_tb.v";
    "mul_by_n.v";
